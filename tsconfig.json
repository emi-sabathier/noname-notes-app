// prettier-ignore
{
  "extends": "@tsconfig/react-native/tsconfig.json",
  /* Recommended React Native TSConfig base */
  "compilerOptions": {
    /* Visit https://aka.ms/tsconfig.json to read more about this file */
    "lib": [
      "ES2017"
    ],
    "target": "es6",
    "module": "commonjs",
    /* Completeness */
    "skipLibCheck": true,
    "typeRoots": [
      "./node_modules/@types",
      "./typings"
    ]
    /* Skip type checking all .d.ts files. */
  }
}
